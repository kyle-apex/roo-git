{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": [],
    "typeRoots": ["./src"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}